import epackage	"inputs/yakindu_simplified.ecore"
import viatra	"src/queries/constraints.vql"

generate {
	metamodel = { package yakindumm }
	constraints = { package queries }
	solver = ViatraSolver
	scope = {
		#node += 12,
		#<yakindumm::Statechart> += 1..1
	}
	
	config = {
		runtime = 180,
		log-level = normal
	}
	
	runs = 1
	number = 1
	
	debug =			"outputs323/debug"
	log =			"outputs323/log.txt"
	output =		"outputs323/models"
	statistics =		"outputs323/stats.csv" 
}
