import epackage	"inputs/rdsSimplified.ecore"
import viatra	"src/queries/constraints.vql"

generate {
	metamodel = { package rdsSimplified }
	constraints = { package queries }
	solver = ViatraSolver
	scope = {
		#node += 16,
		#<rdsSimplified::Database> += 1..1
	}
	
	config = {
		runtime = 180,
		log-level = normal
	}
	
	runs = 1
	number = 1
	
	debug =			"outputs180/debug"
	log =			"outputs180/log.txt"
	output =		"outputs180/models"
	statistics =		"outputs180/stats.csv" 
}
