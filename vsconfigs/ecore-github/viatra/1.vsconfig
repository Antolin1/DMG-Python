import epackage	"inputs/smallEcore.ecore"
import viatra	"src/queries/constraints.vql"

generate {
	metamodel = { package smallEcore }
	constraints = { package queries }
	solver = ViatraSolver
	scope = {
		#node += 18,
		#<smallEcore::EPackage> += 1..1
	}
	
	config = {
		runtime = 180,
		log-level = normal
	}
	
	runs = 1
	number = 1
	
	debug =			"outputs1/debug"
	log =			"outputs1/log.txt"
	output =		"outputs1/models"
	statistics =		"outputs1/stats.csv" 
}
